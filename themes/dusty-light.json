{
  "name": "Dusty Light",
  "type": "light",
  "author": "Dustin",
  "colors": {
    "editorOverviewRuler.wordHighlightStrongForeground": "#FAEBAF",
    "editor.background": "#ffffff",
    "editor.foreground": "#555555",
    "editorCursor.foreground": "#333333",
    "editor.invisibles": "#DDDDDD",
    "editor.selectionBackground": "#d0d0d0",
    "editor.rangeHighlightBackground": "#FDF7DD",
    "editor.lineHighlightBackground": "#FDF7DDBB",
    "editorWhitespace.foreground": "#eeeeee",
    "editorLink.activeForeground": "#80A4BE",
    "editorLineNumber.foreground": "#d0d0d0",
    "activityBarBadge.background": "#3399ff",
    "activityBarBadge.foreground": "#ffffff",
    "list.inactiveSelectionBackground": "#d0d0d0",
    "list.activeSelectionBackground": "#3399ff",
    "notification.buttonBackground": "#eeeeee",
    "notification.buttonForeground": "#555555",
    "notification.buttonHoverBackground": "#d0d0d0",
    "notification.errorBackground": "#FF5555",
    "notification.errorForeground": "#555555",
    "notification.infoBackground": "#8BE9FD",
    "notification.infoForeground": "#282A36",
    "notification.warningBackground": "#FFB86C",
    "notification.warningForeground": "#282A36",
    "peekViewResult.fileForeground": "#555555",
    "peekViewResult.lineForeground": "#555555",
    "scrollbar.shadow": null,
    "welcomePage.buttonBackground": null,
    "welcomePage.buttonHoverBackground": null,
    "widget.shadow": null
  },
  "tokenColors": [
    {
      "scope": [
        "emphasis"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "strong"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Edge cases (foreground color resets)",
      "scope": [
        "constant.other.symbol.hashkey.ruby",
        "keyword.operator.dereference.java",
        "keyword.operator.navigation.groovy",
        "meta.scope.for-loop.shell punctuation.definition.string.begin",
        "meta.scope.for-loop.shell punctuation.definition.string.end",
        "meta.scope.for-loop.shell string",
        "storage.modifier.import",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.separator.list.comma.css",
        "constant.language.empty-list.haskell"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#89995E"
      }
    },
    {
      "name": "Regex & Variables",
      "scope": [
        "string.regexp",
        "variable",
        "support.other.variable",
        "string.other.link",
        "support.variable.property",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#CC5C52"
      }
    },
    {
      "name": "Operators, misc",
      "scope": [
        "keyword.operator.class",
        "source.php.embedded.line"
      ],
      "settings": {
        "foreground": "#639999"
      }
    },
    {
      "name": "Dom & Object variable types",
      "scope": [
        "support.variable.dom",
        "support.variable.property.dom",
        "variable.other.object",
        "variable.other.object.property",
        "variable.other.property",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#42B24D"
      }
    },
    {
      "name": "Constants and Numerics",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "variable.parameter",
        "punctuation.section.embedded",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#DE9E59"
      }
    },
    {
      "name": "Keyword Operators",
      "scope": [
        "keyword.operator",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#82B9BB"
      }
    },
    {
      "name": "Function and special methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method",
        "meta.block-level"
      ],
      "settings": {
        "foreground": "#80A4BE"
      }
    },
    {
      "name": "Keyword & Storage",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "declaration.tag",
        "beginning.punctuation.definition.quote.markdown",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#A184CE"
      }
    },
    {
      "name": "Entity Tags",
      "scope": [
        "entity.name.tag",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#CE5C39"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": [
        "meta.tag.inline.any",
        "meta.tag.any.html",
        "meta.tag.other.html",
        "meta.tag.block.any.html"
      ],
      "settings": {
        "foreground": "#8E7808"
      }
    },
    {
      "name": "Module Name",
      "scope": [
        "entity.name.type.module",
        "entity.name.type",
        "markup.bold.markdown",
        "meta.at-rule.extend",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#DB8734"
      }
    },
    {
      "name": "Bold Class Names",
      "scope": [
        "entity.name.type",
        "keyword.operator.new",
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Separators",
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Invalid - Depricated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#D63F37"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#B3362D"
      }
    }
  ]
}