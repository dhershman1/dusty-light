{
  "name": "Dusty Light",
  "type": "light",
  "colors": {
    "editorOverviewRuler.wordHighlightStrongForeground": "#FAEBAF",
    "editor.background": "#ffffff",
    "editor.foreground": "#555555",
    "editorCursor.foreground": "#333333",
    "editor.invisibles": "#DDDDDD",
    "editor.selectionBackground": "#d0d0d0",
    "editor.rangeHighlightBackground": "#FDF7DD",
    "editor.lineHighlightBackground": "#FDF7DDBB",
    "editorWhitespace.foreground": "#eeeeee",
    "editorLink.activeForeground": "#80A4BE",
    "editorLineNumber.foreground": "#d0d0d0",
    "activityBarBadge.background": "#3399ff",
    "activityBarBadge.foreground": "#ffffff",
    "list.inactiveSelectionBackground": "#d0d0d0",
    "list.activeSelectionBackground": "#3399ff"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#89995E"
      }
    },
    {
      "name": "Regex & Variables",
      "scope": ["string.regexp", "variable", "support.other.variable", "string.other.link", "support.variable.property"],
      "settings": {
        "foreground": "#CC5C52"
      }
    },
    {
      "name": "Operators, misc",
      "scope": [
        "keyword.operator.class", "constant.other", "source.php.embedded.line"
      ],
      "settings": {
        "foreground": "#639999"
      }
    },
    {
      "name": "Dom & Object variable types",
      "scope": [
        "support.variable.dom",
        "support.variable.property.dom",
        "variable.other.object",
        "variable.other.object.property",
        "variable.other.property",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#42B24D"
      }
    },
    {
      "name": "Constants and Numerics",
      "scope": ["constant.numeric", "constant.language", "support.constant", "constant.character", "variable.parameter", "punctuation.section.embedded", "keyword.other.unit"],
      "settings": {
        "foreground": "#DE9E59"
      }
    },
    {
      "name": "Keyword Operators",
      "scope": ["keyword.operator", "constant.other.color"],
      "settings": {
        "foreground": "#82B9BB"
      }
    },
    {
      "name": "Function and special methods",
      "scope": ["entity.name.function", "meta.function-call", "support.function", "keyword.other.special-method", "meta.block-level"],
      "settings": {
        "foreground": "#80A4BE"
      }
    },
    {
      "name": "Keyword & Storage",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "declaration.tag",
        "beginning.punctuation.definition.quote.markdown"
      ],
      "settings": {
        "foreground": "#A184CE"
      }
    },
    {
      "name": "Entity Tags",
      "scope": [
        "entity.name.tag",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#CE5C39"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#8E7808"
      }
    },
    {
      "name": "Module Name",
      "scope": [
        "entity.name.type.module",
        "entity.name.type",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#DB8734"
      }
    },
    {
      "name": "Separators",
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Invalid - Depricated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#D63F37"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#B3362D"
      }
    }
  ]
}